{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES5",
    "outDir": "./out",
    "lib": [
      "ES5",
      "DOM",
      "es2015.promise"
    ],
    "sourceMap": true,
    "rootDir": ".",
    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedParameters": true
  },
  "exclude": [
    "app"
  ],
  "include": [
    "src",
    "app/common"
  ]
}
