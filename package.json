{
  "name": "googlogs",
  "displayName": "Googlogs",
  "description": "Google cloud logs viewer",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "googelogs.explorer"
  ],
  "main": "./out/src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "googelogs.openLogsViewer",
        "title": "View Google Logs",
        "category": "Googelogs"
      }
    ],
    "views": {
      "explorer": [
        {
          "type": "webview",
          "id": "googelogs.view",
          "name": "Googelogs"
        }
      ]
    },
    "keybindings": [
      {
        "command": "googelogs.openLogsViewer",
        "key": "ctrl+l",
        "mac": "cmd+l"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "npm-run-all compile:*",
    "compile:extension": "tsc -p ./",
    "compile:view": "webpack --mode development",
    "watch": "npm-run-all -p watch:*",
    "watch:extension": "tsc -watch -p ./",
    "watch:view": "webpack --watch --mode development",
    "lint": "eslint src --ext ts --fix",
    "browser": "webpack serve --mode development"
  },
  "dependencies": {
    "@react-hook/window-size": "^3.0.7",
    "axios": "^0.27.2",
    "moment": "^2.29.3",
    "object-path": "^0.11.8",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-loader-spinner": "^6.0.0-0",
    "react-router-dom": "^5.2.0",
    "react-select": "^5.3.2",
    "react-tagsinput": "^3.19.0",
    "styled-components": "5.3.5"
  },
  "devDependencies": {
    "@google-cloud/logging": "^10.0.2",
    "@google-cloud/resource-manager": "^3.2.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/object-path": "^0.11.1",
    "@types/react-dom": "18.0.5",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-tagsinput": "^3.19.9",
    "@types/styled-components": "5.1.25",
    "@types/util.promisify": "^1.0.4",
    "@types/vscode": "^1.52.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.15.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.15",
    "typescript": "^4.1.2",
    "vscode-test": "^1.4.1",
    "webpack": "^5.21.1",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^4.9.1"
  }
}
